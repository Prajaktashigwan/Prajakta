    "if len(A)%2 != 0:\n",
    "    I = (len(A)-1)//2\n",
    "    med = A[I]\n",
    "else:\n",
    "    I1 = len(A)//2\n",
    "    I2 = I1-1\n",
    "    med = (A[I1]+A[I2])/2\n",
    "    \n",
    "end=time.time()\n",
    "\n",
    "print('median using sort method: '+str(med))\n",
    "print(\"total time taken by using sort method: \",(end-start)*10**3, \"ms\")"
   ]
